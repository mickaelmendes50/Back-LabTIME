// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  codigo           Int    @id
  nomeCompleto     String
  nomeSocial       String
  dataDeNascimento String
  sexo             String
  email            String @unique
  estado           String
  municipio        String
  numeroDeAcessos  Int
  situacao         String
  dataDeVinculo    String
}
